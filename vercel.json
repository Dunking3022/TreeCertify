{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "preBuild": {
      "runtime": "@now/node",
      "includeFiles": ["setup.sh"]
    },
    "api": {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  }
}
